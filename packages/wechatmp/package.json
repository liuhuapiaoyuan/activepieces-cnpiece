    "docs"
  ],
  "scripts": {
    "build": "bun build ./src/index.ts --outdir=dist --external=@activepieces/pieces-common @activepieces/pieces-framework --format=cjs  --tsconfig=tsconfig.json && tsc --declaration --emitDeclarationOnly ",
